# Example environment variables for the OCR Processor
# Copy this file to `.env` and fill in real values. DO NOT commit your `.env` file.

# Azure Storage connection string (required at runtime)
# e.g. DefaultEndpointsProtocol=https;AccountName=...;AccountKey=...;EndpointSuffix=core.windows.net
AZURE_STORAGE_CONNECTION_STRING=

# Queue names
INPUT_QUEUE_NAME=ocrinputqueue1
CLASSIFICATION_QUEUE_NAME=ocrresponsequeue1

# Local artifacts and logging
ARTIFACTS_DIR=artifacts
LOG_LEVEL=INFO

# Worker / OCR tuning
MAX_WORKERS=10
TESSERACT_TIMEOUT=120

# Blob container name (if used)
BLOB_CONTAINER_NAME=pdfdocuments

# When true the processor will skip calling the API and only queue classification messages
SKIP_API_CALL=false
